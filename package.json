{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "lint-fix": "eslint \"src/**/*.js\" --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylefmt": "stylefmt -r \"src/**/*.scss\"",
    "flow": "flow",
    "api": "nodemon --exec babel-node ./api/server.js",
    "build": "cross-env NODE_ENV=production npm run clean && webpack --config ./webpack/prod.config.js",
    "start": "cross-env NODE_ENV=development npm run clean && webpack-dev-server --config ./webpack/dev.config.js",
    "test-file": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require src/app/test.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require src/app/test.js --recursive \"src/**/*.test.js\"",
    "test-min": "cross-env NODE_ENV=test mocha --reporter min --compilers js:babel-register --require src/app/test.js --recursive \"src/**/*.test.js\"",
    "cover": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --compilers js:babel-register --require src/app/test.js --recursive \"src/**/*.test.js\"",
    "clean": "rimraf build",
    "fmt": "npm run lint-fix & npm run stylefmt",
    "precommit": "npm run lint & npm run flow & npm run stylelint & npm run test-min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nabepon/frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nabepon/frontend/issues"
  },
  "homepage": "https://github.com/nabepon/frontend#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
